SUBDESIGN decode_11a
( 
	data[1..0]	:	input;
	eq[3..0]	:	output;
) 
VARIABLE 
	data_wire[1..0]	: WIRE;
	eq_node[3..0]	: WIRE;
	eq_wire[3..0]	: WIRE;
	w_anode62w[2..0]	: WIRE;
	w_anode76w[2..0]	: WIRE;
	w_anode85w[2..0]	: WIRE;
	w_anode94w[2..0]	: WIRE;

BEGIN 
	data_wire[] = data[];
	eq[] = eq_node[];
	eq_node[3..0] = eq_wire[3..0];
	eq_wire[] = ( w_anode94w[2..2], w_anode85w[2..2], w_anode76w[2..2], w_anode62w[2..2]);
	w_anode62w[] = ( (w_anode62w[1..1] & (! data_wire[1..1])), (w_anode62w[0..0] & (! data_wire[0..0])), B"1");
	w_anode76w[] = ( (w_anode76w[1..1] & (! data_wire[1..1])), (w_anode76w[0..0] & data_wire[0..0]), B"1");
	w_anode85w[] = ( (w_anode85w[1..1] & data_wire[1..1]), (w_anode85w[0..0] & (! data_wire[0..0])), B"1");
	w_anode94w[] = ( (w_anode94w[1..1] & data_wire[1..1]), (w_anode94w[0..0] & data_wire[0..0]), B"1");
END;
